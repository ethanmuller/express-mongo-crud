<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>mush</title>
  </head>

  <body>
    <h1>mush</h1>

    <form action="/passages" method="POST">
      <input type="text" name="name">
      <button type="submit">add room</button>
    </form>
    <h2>rooms</h2>
    <ul>
      <% for(var i = 0; i < passages.length; i++) { %>
        <li><a href="/<%= passages[i]['_id'] %>"><%= passages[i]['name'] || passages[i]['_id'] %></a></li>
      <% } %>
    </ul>
  </body>
</html>
